{
	"name": "country-flags-svg-docs",
	"version": "1.0.0",
	"description": "Docs for a list of countries with url links to a svg image which you can simply use in your web application",
	"source": "src/index.html",
	"files": [
		"dist"
	],
	"scripts": {
		"start": "parcel src/index.html --open",
		"build": "parcel build src/index.html --public-url /country-flags-svg --no-content-hash",
		"clean": "rimraf dist",
		"predeploy": "npm run clean && npm install && npm run build",
		"deploy": "gh-pages -d dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ronatskiy/country-flags-svg.git"
	},
	"author": "Roman Onatskiy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ronatskiy/country-flags-svg/issues"
	},
	"homepage": "https://github.com/ronatskiy/country-flags-svg#readme",
	"devDependencies": {
		"gh-pages": "^3.2.3",
		"parcel": "^2.3.2",
		"rimraf": "^3.0.2"
	},
	"dependencies": {
		"country-flags-svg": "^1.1.8"
	},
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not dead"
	]
}
